<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_focused="true">
        <shape>
            <solid android:color="@color/sodk_editor_palette_tufts_blue"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/sodk_editor_palette_tufts_blue"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/sodk_editor_palette_silver_1"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/sodk_editor_palette_azure"/>
            <corners android:radius="12dp"/>
        </shape>
    </item>
</selector>
